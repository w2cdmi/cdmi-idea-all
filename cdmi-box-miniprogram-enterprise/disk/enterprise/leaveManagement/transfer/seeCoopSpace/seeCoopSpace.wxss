
@import "/disk/template/fileList.wxss";
.coop-space-file{
  width: 100%;
  background: #fff;
  margin-top: 20rpx;
}
.coop-list-title{
  display: flex;
  padding: 15rpx 30rpx;
  font-size: 24rpx;
  border-bottom: 1rpx solid #f5f5f5;
  justify-content: space-between
}
.list-detail-left,.list-detail-right{
  display: flex
}
.coop-title-middle{
  margin: 11rpx 22rpx;
  width: 14rpx;
  height: 14rpx;
  border-top: 1px solid #999999;
  border-right: 1px solid #999999;
  transform: rotate(45deg);
}
.seeCoop-space-confirm{
  width: 92%;
  position: fixed;
  bottom: 54rpx;
  left: 4%;
}
.coop-button-style{
  background: #ea5036;
  color: #fff;
  font-size: 32rpx;
}
.not-list-detail-bg{
    transform:translateY(50%);
}
.not-list-detail-bg image{
  width: 130px;
  height:100px;
}
.coop-space-file{
    height: 100%
}