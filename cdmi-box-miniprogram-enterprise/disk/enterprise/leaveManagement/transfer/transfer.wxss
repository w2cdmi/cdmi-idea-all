@import "/disk/template/fileList.wxss";
@import "/disk/recycle/recycle.wxss";
.transfer-content-bg{
  width: 100%;
  height: 100%;
  background: #fff;
}
.transfer-bg{
  width: 100%;
  height: 20rpx;
  background: #f5f5f5
}
.transfer-title-choose{
  display: flex;
  font-size: 30rpx;
  color: #333;
}
.transfer-choose{
  width: 50%;
  text-align: center;
  padding: 18rpx 0;
}
.transfer-choose-active{
  color: #ea5036;
  border-bottom: 1rpx solid #ea5036;
}
.coop-space-title{
  display: flex;
  padding: 15rpx 30rpx;
  justify-content: space-between;
  color: #666;
  border-bottom: 1rpx solid #f5f5f5
}
.batch-transfer{
  display:flex;
  justify-content:center;
  align-items:center
}
.transfer-icon{
  width: 24rpx;
  height: 25rpx;
  padding-right: 10rpx
}
.border-bottom{
  border-bottom: 1rpx solid #f5f5f5;
}
.per-opt-right{
  display: flex
}
.document-batch-transfer{
  margin-right: 40rpx;
}
.not-list-detail-bg{
  transform:translateY(50%);
}
.not-list-detail-bg image{
  width: 130px;
  height:100px;
}
.coop-name-title{
    line-height: 90rpx;
    height: 90rpx;
}