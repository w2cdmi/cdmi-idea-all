.wechat-backup-page {
    height: 100%;
    width: 100%;
    background: #fff;
    font-size: 28rpx;
    margin: 0;
}

.wechat-bakup-content {
    background-color: #fff;
}

.wechat-backup-top-middle {
    text-align: center;
}

.wechat-backup-top {
    position: relative;
    width: 200rpx;
    height: 200rpx;
    padding-top: 170rpx;
    margin: auto;
    margin-bottom: 130rpx;
}

.wechat-backup-top image {
    position: absolute;
    width: 200rpx;
    height: 200rpx;
    z-index: 4;
}

@keyframes living1 {
    0% {
        transform: scale(1.1);
        opacity: 0;
    }

    /* 20% {
        transform: scale(1.);
        opacity: 1;
    } */

    /* 50% {
        transform: scale(1.3);
        opacity: 1;
    } */

    /* 50% {
        transform: scale(1.5);
        opacity: 1;
    }

    60% {
        transform: scale(1.4);
        opacity: 1;
    }

    80% {
        transform: scale(1.25);
        opacity: 1;
    } */

    100% {
        transform: scale(1.5);
        opacity: 1;
    }
}

@keyframes living2 {
    0% {
        transform: scale(1.5);
        opacity: 0;
    }

    /* 20% {
        transform: scale(1.65);
        opacity: 1;
    }

    40% {
        transform: scale(1.8);
        opacity: 1;
    } */

    /* 50% {
        transform: scale(1.7);
        opacity: 1;
    } */

    /* 60% {
        transform: scale(1.8);
        opacity: 1;
    }

    80% {
        transform: scale(1.65);
        opacity: 1;
    } */

    100% {
        transform: scale(1.9);
        opacity: 1;
    }
}

@keyframes living3 {
    0% {
        transform: scale(1.9);
        opacity: 0;
    }

    /* 20% {
        transform: scale(2.05);
        opacity: 1;
    }

    40% {
        transform: scale(2.2);
        opacity: 1;
    } */

    /* 50% {
        transform: scale(2.1);
        opacity: 1;
    } */

    /* 60% {
        transform: scale(2.2);
        opacity: 1;
    }

    80% {
        transform: scale(2.05);
        opacity: 1;
    } */

    100% {
        transform: scale(2.3);
        opacity: 1;
    }
}

.wechat-backup-top .spread-one {
    position: absolute;
    width: 200rpx;
    height: 200rpx;
    left: 0;
    bottom: 0;
    background: #daeeff;
    border-radius: 50%;
    animation: living1 2s linear infinite;
    z-index: 3;
}

.wechat-backup-top .spread-two {
    position: absolute;
    width: 200rpx;
    height: 200rpx;
    left: 0;
    bottom: 0;
    background: #f0f8ff;
    border-radius: 50%;
    animation: living2 2s linear infinite;
    z-index: 2;
}

.wechat-backup-top .spread-three {
    position: absolute;
    width: 200rpx;
    height: 200rpx;
    left: 0;
    bottom: 0;
    background: #fbfdff;
    border-radius: 50%;
    animation: living3 2s linear infinite;
    z-index: 1;
}

.wechat-backup-title {
    margin-bottom: 26rpx;
}

.wechat-data-storage {
    font-size: 24rpx;
    color: #666;
}

.sure-button {
    margin-top: 100rpx;
}

.wechat-backup-set-button {
    font-size: 28rpx;
    text-align: center;
    margin: 20rpx 30rpx;
    background: #fff;
    height: 40px;
    line-height: 40px;
    border-radius: 5px;
    border: 1px solid #999;
}

.backup-bg {
    height: 20rpx;
    background: #f5f5f5;
}

.backup-set-title {
    font-size: 26rpx;
    padding: 20rpx 30rpx;
    color: #666;
    border-bottom: 1px solid #f5f5f5;
}

.wechat-backup-setlist-con {
    display: flex;
    /* justify-content: space-between; */
    padding: 0 30rpx;
    flex-wrap: wrap;
}

.wechat-group-list {
    width: 210rpx;
    height: 85rpx;
    line-height: 85rpx;
    display: flex;
}
.wechat-group-list+.wechat-group-list{
    margin-left: 4.3%;
}
.wechat-group-list:nth-child(3n+1){
    margin-left: 0
}
.wechat-group-list image {
    width: 50rpx;
    height: 50rpx;
    margin-top: 16rpx;
}

.wechat-group-list text {
    margin-left: 15rpx;
    font-size: 26rpx;
    display: inline-block;
    max-width: 145rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
