/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.all-container{
    height: 100%;
    background: #fff;
}
page{
    font-size: 30rpx;
    background: #f1f1f1; 
    height: 100%;
    width: 750rpx;
    overflow-x: hidden;    
}

.flex-between{
    display: flex;
    justify-content: space-between;
}

.flex-around{
    display: flex;
    justify-content: space-around;
}
